{
  "name": "wetonomy",
  "version": "1.0.0",
  "description": "",
  "dependencies": {
    "@aragon/client": "^1.0.0-beta.8",
    "@aragon/os": "3.1.9",
    "@aragon/ui": "^0.11.0",
    "ethereumjs-testrpc": "^6.0.3",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "solhint": "^1.2.1",
    "styled-components": "^3.2.6"
  },
  "devDependencies": {
    "@aragon/cli": "^4.0.6",
    "babel-plugin-styled-components": "^1.5.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-2": "^6.24.1",
    "lerna": "^2.11.0",
    "parcel-bundler": "^1.8.1",
    "truffle": "^4.1.13"
  },
  "scripts": {
    "bootstrap": "lerna bootstrap --hoist",
    "clean": "git clean -fdxi",
    "start": "aragon run",
    "test": "lerna run test",
    "lint": "solhint apps/**/contracts/*.sol",
    "compile": "aragon contracts compile && npm run lint",
    "sync-assets": "mkdir -p dist; rsync -rtu \"$(dirname $(node -p 'require.resolve(\"@aragon/ui\")'))/\" dist/aragon-ui",
    "build:app": "parcel build app/index.html -d dist/ --public-url '.'",
    "build:script": "parcel build app/script.js -d dist/",
    "build": "npm run compile && npm run sync-assets && npm run build:app && npm run build:script",
    "build:apps": "npm run compile && lerna run --concurrency=1 --stream build",
    "publish:apps": "lerna run --concurrency=1 --stream publish",
    "publish": "aragon publish"
  },
  "keywords": [],
  "license": "GPL-3.0"
}
